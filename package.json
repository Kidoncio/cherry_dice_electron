    "linux": {
      "icon": "icon.png"
    },
    "snap": {
      "category": "Utility"
    }
  },
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^7.1.1",
    "electron-installer-snap": "^4.0.0",
